		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public double longueur() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public void doubler() {
		// TODO Auto-generated method stub

	}

	public double getRayon() {
		return rayon;
	}

	public void setRayon(double rayon) {
		this.rayon = rayon;
	}
	

}
