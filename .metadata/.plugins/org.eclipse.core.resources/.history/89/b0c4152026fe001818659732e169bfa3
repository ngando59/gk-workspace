package gk.ngando.demo.domain;

public class ThreadAffichageCompteur extends Thread {
	private int nbMax;

	public ThreadAffichageCompteur() {
		super();
		this.nbMax = 7;
	}

	public ThreadAffichageCompteur(String name, int nbMax) {
		super(name);
		this.nbMax = nbMax;
	}

	@Override
	public void run() {
		for (int i = 0; i < this.nbMax; i++) {
			
		}
	}

}
