	public boolean estPalindrome(String str) {
		boolean result = true;
		int nbCaracteres = 0;
		for (int i = 0; i < str.length(); i++) { 
			if ((Character.isAlphabetic(c)) || (Character.isDigit(c))) {
				nbCaracteres++;
			}
		}
		
		char[] lettres = new char[str.length()];
		for (int i = 0; i < str.length(); i++) {
			char c = str.charAt(i);
			if ((Character.isAlphabetic(c)) || (Character.isDigit(c))) {
				lettres[i] = c;
			}
		}
		if(lettres)
		return result;
	}

